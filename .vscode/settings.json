{
  "editor.tabSize": 2,
  "files.autoSave": "afterDelay",
  "editor.fontFamily": "'Lucida Console', 'Courier New', Consolas, monospace",
  "editor.accessibilitySupport": "off",
  "files.associations": {
    "*.htm": "html",
    "*.html": "html",
    "*.yml": "yaml",
    "*.yaml": "yaml",
    "*.css": "css",
    "*.json": "json",
    "*.xml": "xml"
  },
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode",
  "git.autofetch": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.tabSize": 2,
    "editor.wordWrap": "wordWrapColumn",
    "editor.accessibilitySupport": "off",
    "editor.codeLens": false
  },
  "editor.indentSize": "tabSize",
  "editor.codeLens": true,
  "editor.codeLensFontFamily": "Lucida Console, Consolas, 'Courier New', monospace",
  "prettier.bracketSameLine": true,
  "yaml.format.proseWrap": "always",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingStrategy": "advanced",
  "html.format.wrapAttributes": "aligned-multiple",
  "html.format.wrapLineLength": 100,
  "editor.renderWhitespace": "all",
  "editor.experimentalWhitespaceRendering": "off"
}
