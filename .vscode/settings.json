{
    "editor.tabSize": 2,
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "'Lucida Console', 'Courier New', Consolas, monospace",
    "editor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "files.associations": {
      "*.htm": "html",
      "*.html": "html",
      "*.yml": "yaml",
      "*.yaml": "yaml",
      "*.css": "css",
      "*.json": "json",
      "*.xml": "xml"
    },
    "editor.detectIndentation": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode"
    "git.autofetch": true
}