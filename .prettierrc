{
  "htmlWhitespaceSensitivity": "strict",
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "printWidth": 80,
  "arrowParens": "always",
  "semi": true,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "singleQuote": false,
  "singleAttributePerLine": false,
  "requirePragma": false,
  "insertPragma": false,
  "useTabs": false
}
